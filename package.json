{
	"name": "botmon",
	"version": "3.0.4",
	"description": "",
	"main": "app.js",
	"scripts": {
		"start": "leo-cli test . -e DEV",
		"old_start": "leo-cli test .",
		"test": "mocha --require ./test/bootstrap.js --recursive -w",
		"debug": "node-debug leo test .",
		"build": "leo-cli publish . --force all --build --save",
		"publish": "leo-cli publish . --force all"
	},
	"config": {
		"leo": {
			"type": "microservice",
			"subtype": "react",
			"role": "ApiRole",
			"name": "Leo_Botmon",
			"display-name": "Bot Monitoring",
			"root": "botmon",
			"runtime": "nodejs20.x",
			"parallelCompile": 10,
			"stacks": [
				"leosdk",
				"leoauth"
			],
			"aws": {
				"cognito_region": {
					"ref": "micro.cognito.region"
				},
				"cognito_id": {
					"ref": "micro.cognito.id"
				}
			},
			"micro": {
				"version": {
					"ref": "micro.version"
				}
			},
			"static": {
				"s3": {
					"ref": "micro.s3.id"
				},
				"cloudfront": {
					"ref": "micro.cloudfront.domain"
				}
			},
			"regions": [
				"us-west-2",
				"us-east-1"
			],
			"showPagesEnv": {
				"Resources": {
					"CognitoId": "${CognitoId}",
					"Region": "${AWS::Region}",
					"CustomJS": "${CustomJS}"
				},
				"Logins": "${Logins}"
			},
			"3rdParty": true,
			"excludeProfiles": true
		},
		"console": {
			"navigation": [
				{
					"title": "Botmon",
					"href": "/botmon",
					"icon": ""
				}
			]
		}
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@aws-sdk/node-http-handler": "^3.370.0",
		"@babel/plugin-proposal-class-properties": "^7.18.6",
		"@babel/plugin-transform-react-jsx": "^7.25.2",
		"@babel/plugin-transform-runtime": "^7.25.4",
		"@emotion/react": "^11.13.3",
		"@emotion/styled": "^11.13.0",
		"@mui/material": "^5.16.7",
		"@uiw/react-codemirror": "^4.23.1",
		"async": "^3.2.6",
		"child_process": "^1.0.2",
		"circular-dependency-plugin": "^5.2.2",
		"css-loader": "^7.1.2",
		"csv-parse": "^5.5.6",
		"d3-jetpack": "^2.2.1",
		"deep-diff": "^1.0.2",
		"elasticsearch": "^16.7.3",
		"extend": "^3.0.2",
		"html5sortable": "^0.14.0",
		"jsonpath": "^1.1.1",
		"later": "^1.2.0",
		"leo-auth": "file:../smartshyp-deps/leo-auth",
		"leo-aws": "file:../smartshyp-deps/leo-aws",
		"leo-config": "file:../smartshyp-deps/leo-config",
		"leo-logger": "^1.0.1",
		"leo-sdk": "file:../smartshyp-deps/leo-sdk",
		"less-loader": "^12.2.0",
		"mobx": "^6.13.1",
		"mobx-react": "^9.1.1",
		"mocha": "^10.7.3",
		"moment": "^2.30.1",
		"moment-round": "^1.0.1",
		"moment-timezone": "^0.5.45",
		"numeral": "^2.0.6",
		"q": "^2.0.3",
		"react": "^18.3.1",
		"react-autobind": "^1.0.6",
		"react-dom": "^18.3.1",
		"react-json-pretty": "^2.2.0",
		"react-redux": "^9.1.2",
		"redux": "^5.0.1",
		"redux-thunk": "^3.1.0",
		"sass-loader": "^16.0.1",
		"style-loader": "^4.0.0",
		"styled-component": "^2.8.0",
		"styled-components": "^6.1.13",
		"uuid": "^10.0.0",
		"webpack-dev-middleware": "^7.4.2",
		"webpack-node-externals": "^3.0.0"
	},
	"overrides": {
		"leo-auth": "file:../smartshyp-deps/leo-auth",
		"leo-sdk": "file:../smartshyp-deps/leo-sdk",
		"leo-aws": "file:../smartshyp-deps/leo-aws",
