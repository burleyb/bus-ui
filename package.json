{
	"name": "botmon",
	"version": "3.0.4",
	"description": "",
	"main": "index.js",
	"private": true,
	"scripts": {
		"start": "leo-cli test . -e DEV",
		"old_start": "leo-cli test .",
		"test": "mocha --require ./test/bootstrap.js --recursive -w",
		"debug": "node-debug leo test .",
		"build": "leo-cli publish . --force all --build --save",
		"publish": "leo-cli publish . --force all",
		"startreact": "react-app-rewired start",
		"buildreact": "react-app-rewired build",
		"testreact": "react-app-rewired test",
		"ejectreact": "react-scripts eject"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"config": {
		"leo": {
			"type": "microservice",
			"subtype": "react",
			"role": "ApiRole",
			"name": "Leo_Botmon",
			"display-name": "Bot Monitoring",
			"root": "botmon",
			"runtime": "nodejs20.x",
			"parallelCompile": 10,
			"stacks": [
				"leosdk",
				"leoauth"
			],
			"aws": {
				"cognito_region": {
					"ref": "micro.cognito.region"
				},
				"cognito_id": {
					"ref": "micro.cognito.id"
				}
			},
			"micro": {
				"version": {
					"ref": "micro.version"
				}
			},
			"static": {
				"s3": {
					"ref": "micro.s3.id"
				},
				"cloudfront": {
					"ref": "micro.cloudfront.domain"
				}
			},
			"regions": [
				"us-east-1"
			],
			"showPagesEnv": {
				"Resources": {
					"CognitoId": "${CognitoId}",
					"Region": "${AWS::Region}",
					"CustomJS": "${CustomJS}"
				},
				"Logins": "${Logins}"
			},
			"3rdParty": true,
			"excludeProfiles": true,
			"Resources": {}
		},
		"console": {
			"navigation": [
				{
					"title": "Botmon",
					"href": "/botmon",
					"icon": ""
				}
			]
		}
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@aws-sdk/client-cloudformation": "^3.674.0",
